{
  "name": "tastebuds",
  "version": "0.0.1",
  "main": "src/index.ts",
  "scripts": {
    "start": "expo start",
    "clean": "rm -rf .expo dist tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --max-warnings 0 .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "npm run typecheck && npm run lint && npm run format:check"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-icons/all-files": "^4.1.0",
    "@react-native-community/checkbox": "^0.5.17",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.4.0",
    "@tamagui/config": "^1.98.3",
    "@tamagui/font-inter": "^1.98.3",
    "@tamagui/lucide-icons": "^1.98.3",
    "axios": "^1.7.2",
    "expo": "~51.0.24",
    "expo-constants": "~16.0.2",
    "expo-font": "~12.0.8",
    "expo-image-picker": "^15.0.7",
    "expo-linear-gradient": "^13.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.20",
    "expo-status-bar": "~1.12.1",
    "firebase": "^10.12.3",
    "openai": "^4.52.7",
    "react": "18.2.0",
    "react-icons": "^5.2.1",
    "react-native": "^0.74.3",
    "react-native-animated-loader": "^1.0.0",
    "react-native-document-picker": "^9.3.0",
    "react-native-google-fonts": "^0.0.0",
    "react-native-image-crop-picker": "^0.41.2",
    "react-native-image-picker": "^7.1.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal": "^13.0.1",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^3.31.1",
    "react-native-svg": "15.2.0",
    "tamagui": "^1.98.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/react": "~18.2.79",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.2.5",
    "react-native-svg-transformer": "^1.4.0",
    "typescript": "~5.3.3"
  },
  "overrides": {
    "react-dom": "18.2.0"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "app.json",
        "options": {
          "parser": "json-stringify"
        }
      }
    ],
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports"
    ],
    "importOrder": [
      "^react$",
      "^react-native$",
      "<THIRD_PARTY_MODULES>",
      "",
      "^[./]"
    ]
  },
  "private": true
}
